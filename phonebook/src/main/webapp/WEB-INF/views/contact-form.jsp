<%@ include file="./header.jsp" %>

<br><br>
<h3>Add a new Contact</h3>

<form method="post">
<div>
Enter First Name :<br> <input type="text" id="firstname" name="firstname" class="btn-block">
<br>
Enter Last Name :<br> <input type="text" id="lastname" name="lastname" class="btn-block">
<br>
Enter Gender :<br> <input type="text" id="gender" name="gender" class="btn-block">
<br>
Enter Email :<br> <input type="text" id="email" name="email" class="btn-block">
<br>
Enter Phone :<br> <input type="text" id="phone" name="phone" class="btn-block">
<br>
Enter City :<br> <input type="text" id="city" name="city" class="btn-block">
<br>
Enter State :<br> <input type="text" id="state" name="state" class="btn-block">
<br>
Enter Country :<br> <input type="text" id="country" name="country" class="btn-block">
<br>
Enter Picture URL :<br> <input type="text" id="picture" name="picture" class="btn-block">
<br><br>
<input type="submit" class="btn btn-primary">

</div>

